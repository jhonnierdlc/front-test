{
  "name": "employee-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers=EdgeAsChrome",
    "test:headless": "ng test --watch=false --browsers=EdgeHeadless",
    "test:coverage": "ng test --code-coverage --watch=false --browsers=EdgeHeadless",
    "test:ci": "ng test --code-coverage --watch=false --browsers=EdgeCI",
    "e2e": "cypress open --browser edge",
    "e2e:headless": "cypress run --browser edge",
    "test:all": "node run-tests.js all",
    "serve:ssr:employee-app": "node dist/employee-app/server/server.mjs",
    "test:e2e": "playwright test --reporter=html"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/jest": "^19.0.1",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.6",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular/cli": "^19.2.6",
    "@angular/compiler-cli": "^19.2.0",
    "@cypress/code-coverage": "^3.14.7",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.18.0",
    "c8": "^10.1.3",
    "cypress": "^15.5.0",
    "jasmine-core": "~5.6.0",
    "jest-environment-jsdom": "^30.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}
